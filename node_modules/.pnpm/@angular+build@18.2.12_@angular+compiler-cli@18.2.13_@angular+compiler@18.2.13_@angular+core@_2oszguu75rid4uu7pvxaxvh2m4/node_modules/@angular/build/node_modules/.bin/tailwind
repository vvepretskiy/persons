#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/lib/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/lib/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules:/home/pc-86/Projects/PesonsData/persons/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../tailwindcss@3.4.16_ts-node@10.9.1_@swc+core@1.5.29_@swc+helpers@0.5.15__@types+node@18.16.9_typescript@5.5.4_/node_modules/tailwindcss/lib/cli.js" "$@"
fi
